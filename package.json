{
  "name": "flirtshaala",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "web": "webpack serve --mode development",
    "start": "npx react-native start",
    "build:web": "webpack --mode production",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android-debug": "cd android && ./gradlew assembleDebug",
    "build:ios": "cd ios && xcodebuild -workspace FlirtShaala.xcworkspace -scheme FlirtShaala -configuration Release -destination generic/platform=iOS -archivePath FlirtShaala.xcarchive archive",
    "clean": "npx react-native clean",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "deploy:web": "npm run build:web && echo 'Deploy dist/ folder to your web hosting service'",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "setup:ios": "npx react-native-permissions ios && cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-camera-roll/camera-roll": "^7.4.0",
    "@react-native-community/netinfo": "^11.2.1",
    "@react-native-google-signin/google-signin": "^10.1.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@supabase/supabase-js": "^2.39.3",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-device-info": "^10.13.0",
    "react-native-document-picker": "^9.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-google-mobile-ads": "^14.3.1",
    "react-native-image-picker": "^7.1.0",
    "react-native-keychain": "^8.2.0",
    "react-native-permissions": "^4.1.5",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.19.10",
    "react-dom": "18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@types/react": "^18.2.6",
    "@types/uuid": "^9.0.8",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-native-web": "^0.19.10",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.77.0",
    "typescript": "5.0.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=18"
  }
}